{
  "name": "blue-archive-story-viewer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "prettier --config .prettierrc.cjs -uw ./src/ ./CICD/ ./public/config/yaml/ *.md",
    "build:json": "node ./CICD/build_json.js",
    "build": "pnpm format; pnpm build:json; vue-tsc && vite build",
    "preview": "pnpm build; vite preview --host",
    "check": "eslint --config ./.eslintrc.json ./src/ ./CICD/ --ext .js,.ts,.vue"
  },
  "dependencies": {
    "axios": "1.1.3",
    "fastest-levenshtein": "1.0.16",
    "pinia": "2.0.23",
    "pinia-plugin-persistedstate": "2.3.0",
    "vue": "3.2.44",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.2.0",
    "@trivago/prettier-plugin-sort-imports": "3.4.0",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "@vitejs/plugin-legacy": "2.3.1",
    "@vitejs/plugin-vue": "3.2.0",
    "@vue/eslint-config-typescript": "11.0.2",
    "chalk": "5.1.2",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort-exports": "0.7.0",
    "eslint-plugin-vue": "9.7.0",
    "js-yaml": "4.1.0",
    "postcss": "8.4.19",
    "prettier": "2.7.1",
    "prettier-plugin-css-order": "1.3.0",
    "rollup-plugin-visualizer": "5.8.3",
    "sass": "1.56.1",
    "terser": "5.15.1",
    "typescript": "4.8.4",
    "vite": "3.2.3",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pwa": "0.13.3",
    "vue-tsc": "1.0.9"
  }
}
