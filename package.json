{
  "name": "blue-archive-story-viewer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "format": "prettier --config .prettierrc.cjs -uw ./src/ ./CICD/ ./public/config/yaml/ *.md",
    "build:json": "node ./CICD/build_json.js",
    "build": "pnpm format; pnpm build:json; vue-tsc && vite build",
    "build:github": "pnpm build; cp ./dist/index.html ./dist/404.html",
    "preview": "pnpm build; vite preview --host",
    "check": "eslint --config ./.eslintrc.json ./src/ ./CICD/ --ext .js,.ts,.vue"
  },
  "dependencies": {
    "@vue/compiler-sfc": "3.x",
    "axios": "1.2.1",
    "axios-retry": "^3.3.1",
    "fastest-levenshtein": "1.0.16",
    "pinia": "2.0.28",
    "pinia-plugin-persistedstate": "3.0.1",
    "vue": "3.2.45",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.2.0",
    "@trivago/prettier-plugin-sort-imports": "4.0.0",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "@vitejs/plugin-legacy": "2.3.1",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/eslint-config-typescript": "11.0.2",
    "chalk": "5.2.0",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort-exports": "0.8.0",
    "eslint-plugin-vue": "9.8.0",
    "js-yaml": "4.1.0",
    "postcss": "8.4.19",
    "prettier": "2.8.1",
    "prettier-plugin-css-order": "1.3.0",
    "rollup-plugin-visualizer": "5.8.3",
    "sass": "1.56.2",
    "terser": "5.16.1",
    "typescript": "4.9.4",
    "vite": "4.0.0",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pwa": "0.13.3",
    "vue-tsc": "1.0.12"
  }
}
