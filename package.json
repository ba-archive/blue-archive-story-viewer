{
  "name": "blue-archive-story-viewer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite build; vite preview --host",
    "check": "pnpx eslint --config ./.eslintrc.json ./src/ --ext .js,.vue",
